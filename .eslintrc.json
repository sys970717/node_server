{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "standard",
        "airbnb-base",
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "indent": [
            "error", 4
        ],
        "no-trainling-spaces": 0,
        "keyworkd-spacing": 0,
        "no-unused-vars": 1,
        "eol-last": 0,
        "max-length": 0
    }
}